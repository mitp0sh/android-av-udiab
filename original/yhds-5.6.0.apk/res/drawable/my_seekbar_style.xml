<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ff85a5b7" android:endColor="#ff85a5b7" android:angle="270.0" android:centerY="0.45" android:centerColor="#ff85a5b7" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff79c10f" android:endColor="#ff79c10f" android:angle="270.0" android:centerY="0.45" android:centerColor="#ff79c10f" />
            </shape>
        </clip>
    </item>
</layer-list>